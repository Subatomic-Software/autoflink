{
  "kafkaSource": {
    "broker": "localhost:9092",
    "topic": "tester",
    "groupid": "test",
    "schema": "Tester",
    "filter": {
      "comp": {
        "field": "key",
        "comp": "neq",
        "val": "key0"
      },
      "filter": {
        "comp": {
          "field": "value",
          "comp": "lt",
          "val": 5
        },
        "sink": {
          "print": "filter1"
        }
      }
    }
  }
}